<p id="notice"><%= notice %></p>

<h1>Listing Product Owners</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_owners.each do |product_owner| %>
      <tr>
        <td><%= link_to 'Show', product_owner %></td>
        <td><%= link_to 'Edit', edit_product_owner_path(product_owner) %></td>
        <td><%= link_to 'Destroy', product_owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product owner', new_product_owner_path %>
